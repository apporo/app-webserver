{
  "name": "app-webserver",
  "version": "0.2.2",
  "description": "Devebot webserver layerware",
  "homepage": "https://github.com/apporo/app-webserver.git",
  "author": {
    "name": "apporo",
    "email": "apporo@devebot.com",
    "url": "https://github.com/apporo"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "bin/",
    "config/",
    "data/",
    "lib/"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
    "clean": "rm -rf ./lib",
    "prepublish": "npm run clean && NODE_ENV=${DEVEBOT_NODE_ENV:-production} npm run build",
    "pretest": "npm run clean && NODE_ENV=${DEVEBOT_NODE_ENV:-test} npm run build",
    "test": "cucumber-js ./test/**/*.feature",
    "verify": "npm run prepublish && cucumber-js ./test/**/*.feature"
  },
  "keywords": [
    "devebot",
    "devebot-plugin"
  ],
  "peerDependencies": {
    "devebot": "^0.2.6"
  },
  "dependencies": {
    "devebot": "^0.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-minify-mangle-names": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "cucumber": "^4.0.0"
  }
}
